{
  "name": "@osn/common",
  "version": "0.0.9",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "rm -rf es && NODE_ENV=production BABEL_ENV=es babel src --out-dir es --copy-files",
    "build:cjs": "rm -rf cjs && NODE_ENV=production BABEL_ENV=cjs babel src --out-dir cjs --copy-files",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "transformIgnorePatterns": []
  },
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./cjs/index.js"
    },
    "./extension": {
      "import": "./es/extension.js",
      "require": "./cjs/extension.js"
    }
  },
  "sideEffects": false,
  "files": [
    "es",
    "cjs",
    "*.js",
    "!test"
  ],
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "@osn/consts": "^0.0.2",
    "@osn/provider-options": "^1.0.2"
  },
  "peerDependencies": {
    "@polkadot/api": "^7.9.1",
    "@polkadot/extension-dapp": "^0.43.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11"
  }
}
